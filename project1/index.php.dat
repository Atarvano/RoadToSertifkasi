<?php

$today = date('Y-m-d', +strtotime('+1 day'));
$date = date('2025-10-28');
echo "Today's date is: " . $today;
echo "\n";
echo "The fixed date is: " . $date;
echo "\n";
echo strtotime('1 day') . "\n";
$op = (strtotime($date) - strtotime($today)) / (86400);
echo "The difference in days is: " . $op;
echo "\n";
echo date("d F Y", strtotime('2025-10-21'));

?>